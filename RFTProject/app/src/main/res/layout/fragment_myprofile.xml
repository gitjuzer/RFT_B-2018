        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bottom_guideline6"
        app:layout_constraintGuide_percent="0.7"
        android:orientation="horizontal"/>

    <Button
        android:id="@+id/newpasswordbutton"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:fontFamily="fantasy"
        android:layout_gravity="center"
        android:textSize="14dp"
        android:text="@string/changepassword"
        android:background="@drawable/buttoncorner"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/bottom_guideline6"
        app:layout_constraintLeft_toLeftOf="@+id/left_guideline6"
        app:layout_constraintRight_toRightOf="@+id/right_guideline6"
        app:layout_constraintTop_toTopOf="@+id/top_guideline6"/>
